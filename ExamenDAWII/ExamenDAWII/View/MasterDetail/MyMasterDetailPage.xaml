<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Agenda.View.MasterDetail.MyMasterDetailPage"
    xmlns:pages="clr-namespace:Agenda.View.MasterDetail"
    xmlns:pagess="clr-namespace:Agenda.View.Tabbed">

    <MasterDetailPage.Master>
        <pages:MyMasterDetailPageMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pagess:Inicio/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>
